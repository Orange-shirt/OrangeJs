function click_powerˉqiandao() {
    if (text("开始种福果").exists()) {
        var xo = 0.32407 * width;
        var yo = 0.390625 * height;
        click(xo, yo);
        var time = 10;
        for (time == 10; time > 0; time--) {
            toastLog("您为首次进入活动请选择一个心怡的奖品！\n" + time + "后将继续运行，若不选择则默认第一个奖品");
            sleep(2000);
        }
        if (text("开始种福果").exists()) {
            var ZFG = text("开始种福果").findOne().bounds();
            click(ZFG.centerX(), ZFG.centerY());
            sleep(2000);
        }
    }
    if (text("开心收下").exists()) {
        var KXSX = text("开心收下").findOne().bounds();
        click(KXSX.centerX(), KXSX.centerY());
        sleep(2000);
    }
    if (text("立即查看").exists()) {
        var KXSX = text("开心收下").findOne().bounds();
        click(KXSX.centerX(), KXSX.centerY());
        toastLog("已点击“立即查看”按钮")
        sleep(2000);
        toastLog("尝试返回活动界面");
        Justback();
        sleep(2000);
    }
    if (text("收下祝福").exists()) {
        var KXSX = text("收下祝福").findOne().bounds();
        click(KXSX.centerX(), KXSX.centerY());
        sleep(2000);
    }
    if (text("知道了").exists()) {
        var ZDL = text("知道了").findOne().bounds();
        click(ZDL.centerX(), ZDL.centerY());
        sleep(2000);
    }
    if (text("去集福气").exists()) {
        var QJFQ = text("去集福气").findOne().bounds();
        click(QJFQ.centerX(), QJFQ.centerY());
        sleep(2000);
    }
    //点击活动入口
    var S = className("android.view.View").depth(20).findOnce();
    if (S != null) {
        var b = S.parent().bounds();
        click(b.right - 50, b.bottom - 50);
        toastLog("已尝试点击“集福气”按钮\n请稍等……");
        sleep(3000);
    } else {
        toastLog("找不到“集福气”按钮");
        sleep(2000);
    }
    sleep(2000);

    if (text("去签到").exists()) {
        var a = text("去签到").findOne().bounds();
        click(a.centerX(), a.centerY());
        sleep(2000);
    }
    if (text("已签到").exists()) {
        toastLog("已处于活动中……");
    } else {
        toastLog("未处于活动界面");
        var A = className("android.webkit.WebView").findOnce(0);
        if (A != null) {
            if (A.text() != "天猫农场-福年种福果") {
                //判断为点错且进入了其它界面，返回重点
                toastLog("点错且进入了其它界面，返回重点");
                Justback();
                sleep(2000);
                //判断是否返回了界面
                var A = className("android.webkit.WebView").findOnce(0);
                if (A != null) {
                    //存在WebView
                    if (A.text() != "天猫农场-福年种福果") {
                        log("返回失败，直接重进");
                        openTBandsearch();
                        jinhuodong();
                        //再尝试重点
                        //重点
                        if (width == 1080) {
                            click(978, 1620);
                        } else if (width == 1440) {
                            click(1299, 2186);
                        } else {
                            //点击入口按钮
                            var x = 0.908 * width;
                            var y = 0.8473958 * height;
                            click(x, y);
                        }
                    } else {
                        log("成功返回活动界面，重点");
                        if (width == 1080) {
                            click(978, 1620);
                        } else if (width == 1440) {
                            click(1299, 2186);
                        } else {
                            //点击入口按钮
                            var x = 0.908 * width;
                            var y = 0.8473958 * height;
                            click(x, y);
                        }
                    }

                } else {
                    log("没有WebView，重进");
                    openTBandsearch();
                    jinhuodong();
                    //重点
                    if (width == 1080) {
                        click(978, 1620);
                    } else if (width == 1440) {
                        click(1299, 2186);
                    } else {
                        //点击入口按钮
                        var x = 0.908 * width;
                        var y = 0.8473958 * height;
                        click(x, y);
                    }
                }
            } else {
                //点错但还在界面，重点！
                //重点!
                toastLog("点错但还在界面，重点！");
                //点击入口按钮
                if (width == 1080) {
                    click(978, 1620);
                } else if (width == 1440) {
                    click(1299, 2186);
                } else {
                    //点击入口按钮
                    var x = 0.908 * width;
                    var y = 0.8473958 * height;
                    click(x, y);
                }
            }
        } else {
            //点错且进入了其它界面，返回重点
            Justback();
            //判断是否返回了界面
            var A = className("android.webkit.WebView").findOnce(0);
            if (A != null) {
                //存在WebView
                if (A.text() != "天猫农场-福年种福果") {
                    log("返回失败，直接重进");
                    openTBandsearch();
                    jinhuodong();
                    //再尝试重点
                    //重点
                    if (width == 1080) {
                        click(978, 1620);
                    } else if (width == 1440) {
                        click(1299, 2186);
                    } else {
                        //点击入口按钮
                        var x = 0.908 * width;
                        var y = 0.8473958 * height;
                        click(x, y);
                    }
                } else {
                    log("成功返回活动界面，重点");
                    if (width == 1080) {
                        click(978, 1620);

                    } else if (width == 1440) {
                        click(1299, 2186);
                    } else {
                        //点击入口按钮
                        var x = 0.908 * width;
                        var y = 0.8473958 * height;
                        click(x, y);
                    }
                }

            }
        }
    }
}
main_js_clickAll15s();

function Justback() {
    //💝💝💝💝💝使用用户设定的返回方法
    if (context_i_back == 0) {
        sleep(1000);
        toastLog("使用普通的返回");
        back();
        sleep(2000);
    }
    if (context_i_back == 1) {
        sleep(1000);
        toastLog("使用ROOT返回\n请确保已给ROOT权限！");
        Back();
        sleep(2000);
    }
    if (context_i_back == 2) {
        openTBandsearch();
        sleep(3000);
        if (currentActivity() != "com.taobao.browser.BrowserActivity") {
            openTBandsearch();
            sleep(3000);
            if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                openTBandsearch_mine();
            }
        }
        jinhuodong();
        click_powerˉqiandao();
    }
    if (context_i_back == 3) {
        sleep(1000);
        toastLog("从屏幕中间向从左向内滑动来返回");
        gestures([context_gestures_speed, [0, height / 2],
            [500, height / 2]
        ]);
        sleep(2000);
    }
    if (context_i_back == 5) {
        sleep(1000);
        toastLog("从屏幕左侧下方向上滑动来返回");
        gestures([context_gestures_speed, [width / 2 - 300, height - 1],
            [width / 2 - 300, height - 500]
        ]);
        sleep(2000);
    }
    if (context_i_back == 4) {
        sleep(1000);
        toastLog("从屏幕中间向从右向内滑动来返回");
        gestures([context_gestures_speed, [width - 1, height / 2],
            [width - 500, height / 2]
        ]);
        sleep(2000);
    }
    if (context_i_back == 6) {
        sleep(1000);
        toastLog("从屏幕左侧下面向上面滑动来返回");
        gestures([context_gestures_speed, [width / 2 + 300, height - 1],
            [width / 2 + 300, height - 500]
        ]);
        sleep(2000);
    }
}

function main_js_clickAll15s() {
    sleep(2000);
    if (text("去签到").exists()) {
        text("去签到").findOne().click();
        sleep(2000);
    }

    if (text("去签到").exists()) {
        var QD = text("去签到").findOne().bounds();
        click(QD.centerX(), QD.centerY());
        toastLog("已尝试点击“去签到”按钮\n" + QD.centerX(), QD.centerY());
        sleep(2000);
    }
    if (className("android.view.View").text("去签到").depth(19).findOnce() != null) {
        var MN = className("android.view.View").text("已签到").depth(19).findOnce().bounds();
        click(MN.centerX(), MN.centerY());
        toastLog("已尝试点击“去签到”按钮");
        sleep(2000);
    }
    if (text("已签到").exists()) {
        toastLog("已处于活动界面");
    } else {
        openTBandsearch();
        sleep(3000);
        if (currentActivity() != "com.taobao.browser.BrowserActivity") {
            openTBandsearch_mine();
        };
        jinhuodong();
        click_powerˉqiandao();
    }
    if (className("android.view.View").text("去领取").findOnce() != null) {
        var Fw = className("android.view.View").text("去领取").findOnce().bounds();
        click(Fw.centerX(), Fw.centerY());
        toastLog("已尝试点击“去领取”按钮");
        sleep(2000);
    }
    while (textContains("去浏览").exists()) {
        textContains("去浏览").findOne().click();
        sleep(3000);
        if (currentActivity() == "com.taobao.tao.TBMainActivity") {
            sleep(3000);
            if (text("每日首次点击频道卡片，领惊喜能量值").exists()) {
                sleep(1000);
                if (text("支付宝").exists()) {
                    var O = text("支付宝").findOne().bounds();
                    click(O.centerX(), O.centerY());
                    openTBandsearch();
                    sleep(3000);
                    if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                        openTBandsearch_mine();
                    }
                    jinhuodong();

                    click_powerˉqiandao();

                } else if (text("金币庄园").exists()) {
                    var O = text("金币庄园").findOne().bounds();
                    click(O.centerX(), O.centerY());
                    openTBandsearch();
                    sleep(3000);
                    if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                        openTBandsearch_mine();
                    }
                    jinhuodong();

                    click_powerˉqiandao();

                } else if (text("天猫榜单").exists()) {
                    var O = text("天猫榜单").findOne().bounds();
                    click(O.centerX(), O.centerY());
                    openTBandsearch();
                    sleep(3000);
                    if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                        openTBandsearch_mine();
                    }
                    jinhuodong();

                    click_powerˉqiandao();

                }
            } //判断是不是在进行“去我的淘宝点击访问频道(0/1) 点击频道得10000能量 去浏览”任务
            if (text("猜你喜欢").exists()) {
                var dengdai10s = 10;
                for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
                    toastLog("正在完成“猜你喜欢浏览15秒”任务\n剩余" + dengdai10s + "秒……")
                    sleep(2000);
                    swipe(width / 2, height - 600, width / 2, 0, 500);
                }
                openTBandsearch();
                sleep(3000);
                if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                    openTBandsearch_mine();
                }
                jinhuodong();

                click_powerˉqiandao();

            } //判断是不是在进行浏览15秒“猜你喜欢”页面
            else {
                var dengdai10s = 10;
                for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
                    toastLog("正在完成“首页下滑浏览商品15秒”任务\n剩余" + dengdai10s + "秒……")
                    sleep(2000);
                    swipe(width / 2, height - 600, width / 2, 0, 500);
                }
                openTBandsearch();
                sleep(3000);
                if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                    openTBandsearch_mine();
                }
                jinhuodong();
                click_powerˉqiandao();
            }
        } else if (currentActivity() == "com.taobao.browser.MultiBrowserActivity") {

            var dengdai10s = 10;
            for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
                toastLog("正在完成“去看看淘宝人生”任务\n等待加载剩余" + dengdai10s + "秒……")
                sleep(2000);
            }
            toastLog("已完成“淘宝人生”任务");
            openTBandsearch();
            sleep(3000);
            if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                openTBandsearch_mine();
            }
            jinhuodong();

            click_powerˉqiandao();

        } else {
            var dengdai10s = 10;
            for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
                toastLog("正在完成“浏览15秒”任务\n剩余" + dengdai10s + "秒……")
                sleep(2000);
                swipe(width / 2, height - 600, width / 2, 0, 500);
            }
            if (currentActivity() == "com.taobao.tao.TBMainActivity") {
                openTBandsearch();
                sleep(3000);
                if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                    openTBandsearch_mine();
                }
                jinhuodong();

                click_powerˉqiandao();

            } else {
                Justback();
                sleep(2000);
                if (text("已签到").exists()) {
                    toastLog("已返回活动界面");
                } else {
                    toastLog("未能返回任务界面，再次返回");
                    Justback();
                    sleep(2000);
                    if (text("已签到").exists()) {
                        toastLog("已处于活动界面");
                    } else {
                        openTBandsearch();
                        sleep(3000);
                        if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                            openTBandsearch_mine();
                        }
                        jinhuodong();
                        click_powerˉqiandao();
                    }
                }
            }
        }
    }
    while (textContains("去搜索").exists()) {
        textContains("去搜索").findOne().click();
        var dengdai10s = 10;
        for (dengdai10s == 10; dengdai10s > 0; dengdai10s--) {
            toastLog("正在完成“去搜索”任务\n剩余" + dengdai10s + "秒……")
            sleep(2000);
