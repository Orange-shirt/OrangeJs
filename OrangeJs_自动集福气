function openTBandsearch() {
    app.startActivity({
        action: "android.intent.action.MAIN",
        packageName: "com.taobao.taobao",
        className: "com.taobao.tao.welcome.Welcome",
        category: ["android.intent.category.LAUNCHER"],
        flags: ["activity_new_task"]
    });
    waitForPackage("com.taobao.taobao");
    toastLog("正在打开淘宝活动页面……");
    /*
    app.startActivity({
        action: "android.intent.action.VIEW",
        packageName: "com.taobao.taobao",
        className: "com.taobao.browser.BrowserActivity",
        data: app.parseUri("taobao://pages.tmall.com/wow/hdwk/act/191212-main"),
        flags: ["grant_read_uri_permission", "grant_write_uri_permission"],
    });
    sleep(3000);
    toastLog("若长时间没有打开活动页面\n请自行打开淘宝至活动界面！");
    */
    openTBandsearch_mine();
}

function openTBandsearch_mine() {
    app.startActivity({
        action: "android.intent.action.VIEW", //此处可为其他值
        packageName: "com.taobao.taobao",
        className: "com.taobao.search.searchdoor.SearchDoorActivity"
        //此处可以加入其他内容，如data、extras
    });
    sleep(2000);
    toastLog("若长时间没有打开淘宝搜索\n请自行打开淘宝至搜索界面！");
    var xi = 0.458 * width;
    var yi = 0.078125 * height;
    click(xi, yi);
    sleep(2000);
    //waitForActivity("com.taobao.browser.BrowserActivity", period = 200);
    toastLog("等待搜索按钮！若长时间停在此处，可关闭淘宝后再打开至搜索界面即可")
    text("搜索").waitFor();
    if (text("搜索").exists()) {
        sleep(1000);
        setText(GJCwords);
        sleep(2000);
        text("搜索").findOne().click();
    }
}

