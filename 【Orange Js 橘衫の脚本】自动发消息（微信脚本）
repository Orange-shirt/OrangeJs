function OpenWXcontent() {
    while (true) {
        if (id("android:id/text1").text("é€šè®¯å½•").findOnce() != null) {
            toastLog("å·²å¤„äºâ€œå¾®ä¿¡é€šè®¯å½•â€ç•Œé¢");
            break;
        } else if (id("com.tencent.mm:id/d9a").className("android.widget.TextView").text("é€šè®¯å½•").findOnce() != null) { //7.0.4
            var A = id("com.tencent.mm:id/d9a").className("android.widget.TextView").text("é€šè®¯å½•").findOnce().bounds();
            click(A.centerX(), A.centerY());
            toastLog("å·²å°è¯•ç‚¹å‡»â€œé€šè®¯å½•â€æŒ‰é’®");
            sleep(2000);
            //break;
        } else if (id("dkb").className("android.widget.TextView").text("é€šè®¯å½•").findOnce() != null) { //7.0.10
            id("dkb").className("android.widget.TextView").text("é€šè®¯å½•").findOnce().parent().parent().click();
            toastLog("å·²å°è¯•ç‚¹å‡»â€œé€šè®¯å½•â€æŒ‰é’®");
            sleep(2000);
        } else if (currentPackage() == "com.tencent.mm") {
            Justback();
            toastLog("å·²å¤„äºå¾®ä¿¡ä¸­ä½†éä¸»ç•Œé¢\næ­£åœ¨å°è¯•è¿”å›ä¸»ç•Œé¢");
            sleep(2000);
        } else {
            app.startActivity({
                action: "android.intent.action.VIEW", //æ­¤å¤„å¯ä¸ºå…¶ä»–å€¼
                packageName: "com.tencent.mm",
                className: "com.tencent.mm.ui.LauncherUI"
                //æ­¤å¤„å¯ä»¥åŠ å…¥å…¶ä»–å†…å®¹ï¼Œå¦‚dataã€extras
            });
            toastLog("å½“å‰æœªå¤„äºå¾®ä¿¡ä¸­\næ­£åœ¨é‡æ–°æ‰“å¼€å¾®ä¿¡");
            sleep(2000);
        }
    }
}


function Doit() {
    while (true) {
        if (className("android.view.ViewGroup").id("com.tencent.mm:id/k1").findOnce() != null) {
            className("android.view.ViewGroup").id("com.tencent.mm:id/k1").findOnce().child(1).child(0).click();
            toastLog("å·²å°è¯•ç‚¹å‡»â€œæœç´¢â€æŒ‰é’®");
            sleep(2000);
            if (className("android.widget.EditText").id("com.tencent.mm:id/l3").findOnce() != null) {
                className("android.widget.EditText").id("com.tencent.mm:id/l3").findOnce().setText(DX);
                toastLog("å·²è®¾ç½®æœç´¢æ–‡å­—")
                sleep(2000);
                if (id("com.tencent.mm:id/qm").text(DX).findOnce() != null) {
                    id("com.tencent.mm:id/qm").text(DX).findOnce().parent().parent().parent().parent().click();
                    toastLog("å·²å°è¯•ç‚¹å‡»è®¾å®šçš„â€œè”ç³»å¯¹è±¡â€")
                    sleep(2000);
                    if (className("android.widget.FrameLayout").descContains(DX).findOnce() != null) {
                        if (id("com.tencent.mm:id/ami").findOnce() != null) {
                            id("com.tencent.mm:id/ami").findOnce().setText(XX);
                            sleep(1000);
                            if (id("com.tencent.mm:id/amp").findOnce() != null) {
                                id("com.tencent.mm:id/amp").findOnce().click();
                                toastLog("å·²ç‚¹å‡»â€œå‘é€â€æŒ‰é’®");
                                device.cancelKeepingAwake();
                                dialogs.alert("è„šæœ¬å·²è¿è¡Œå®Œæˆ");
                                log("è„šæœ¬å·²è¿è¡Œå®Œæˆ");
                                exit();
                                break;
                            } else {
                                toastLog("å½“å‰ç•Œé¢æ‰¾ä¸åˆ°æ¶ˆæ¯å‘é€æŒ‰é’®\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                                sleep(2000);
                                OpenWXcontent();
                            }
                        } else {
                            toastLog("å½“å‰ç•Œé¢æ‰¾ä¸åˆ°æ¶ˆæ¯è¾“å…¥æ¡†\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                            sleep(2000);
                            OpenWXcontent();
                        }
                    } else {
                        toastLog("å½“å‰éæ­£ç¡®çš„è”ç³»ç•Œé¢\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                        sleep(2000);
                        OpenWXcontent();
                    }
                } else {
                    var WrongDX = 0;
                    WrongDX++;
                    toastLog("å½“å‰ç•Œé¢æœªæ‰¾åˆ°è®¾å®šçš„è”ç³»åç§°\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                    if (WrongDX >= 3) {
                        dialogs.alert("æ— æ³•æ‰¾åˆ°é…ç½®çš„è”ç³»åç§°\nè¯·ç¡®è®¤æ‚¨è¾“å…¥çš„è”ç³»åç§°æ˜¯å¦æ­£ç¡®", "åœ¨è¾“å…¥è”ç³»åç§°æ—¶è¯·ç¡®ä¿å®Œå…¨æ­£ç¡®ï¼Œæ‚¨å¯é‡æ–°è¿è¡Œè„šæœ¬å¹¶åœ¨è„šæœ¬é…ç½®ä¸­ä¿®æ”¹è”ç³»åç§°é…ç½®");
                        exit();
                    }
                    OpenWXcontent();
                }
            } else {
                toastLog("å½“å‰ç•Œé¢æœªæ‰¾åˆ°æœç´¢æ¡†\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                OpenWXcontent();
            }
        } else if (className("android.view.ViewGroup").id("com.tencent.mm:id/l2").findOnce() != null) { //7.0.10
            className("android.view.ViewGroup").id("com.tencent.mm:id/l2").findOnce().child(1).child(0).click();
            toastLog("å·²å°è¯•ç‚¹å‡»â€œæœç´¢â€æŒ‰é’®");
            sleep(2000);
            if (className("android.widget.EditText").id("com.tencent.mm:id/m7").findOnce() != null) {
                className("android.widget.EditText").id("com.tencent.mm:id/m7").findOnce().setText(DX);
                toastLog("å·²è®¾ç½®æœç´¢æ–‡å­—")
                sleep(2000);
                if (id("com.tencent.mm:id/s7").text(DX).findOnce() != null) {
                    id("com.tencent.mm:id/s7").text(DX).findOnce().parent().parent().parent().parent().click();
                    toastLog("å·²å°è¯•ç‚¹å‡»è®¾å®šçš„â€œè”ç³»å¯¹è±¡â€")
                    sleep(2000);
                    if (id("com.tencent.mm:id/aqe").findOnce() != null) {
                        id("com.tencent.mm:id/aqe").findOnce().setText(XX);
                        sleep(1000);
                        if (id("com.tencent.mm:id/aql").findOnce() != null) {
                            id("com.tencent.mm:id/aql").findOnce().click();
                            toastLog("å·²ç‚¹å‡»â€œå‘é€â€æŒ‰é’®");
                            device.cancelKeepingAwake();
                            dialogs.alert("è„šæœ¬å·²è¿è¡Œå®Œæˆ");
                            log("è„šæœ¬å·²è¿è¡Œå®Œæˆ");
                            exit();
                            break;
                        } else {
                            toastLog("å½“å‰ç•Œé¢æ‰¾ä¸åˆ°æ¶ˆæ¯å‘é€æŒ‰é’®\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                            sleep(2000);
                            OpenWXcontent();
                        }
                    } else {
                        toastLog("å½“å‰éæ­£ç¡®çš„è”ç³»ç•Œé¢\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                        sleep(2000);
                        OpenWXcontent();
                    }
                } else {
                    var WrongDX = 0;
                    WrongDX++;
                    toastLog("å½“å‰ç•Œé¢æœªæ‰¾åˆ°è®¾å®šçš„è”ç³»åç§°\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                    if (WrongDX >= 3) {
                        dialogs.alert("æ— æ³•æ‰¾åˆ°é…ç½®çš„è”ç³»åç§°\nè¯·ç¡®è®¤æ‚¨è¾“å…¥çš„è”ç³»åç§°æ˜¯å¦æ­£ç¡®", "åœ¨è¾“å…¥è”ç³»åç§°æ—¶è¯·ç¡®ä¿å®Œå…¨æ­£ç¡®ï¼Œæ‚¨å¯é‡æ–°è¿è¡Œè„šæœ¬å¹¶åœ¨è„šæœ¬é…ç½®ä¸­ä¿®æ”¹è”ç³»åç§°é…ç½®");
                        exit();
                    }
                    OpenWXcontent();
                }
            } else {
                toastLog("å½“å‰ç•Œé¢æœªæ‰¾åˆ°æœç´¢æ¡†\næ­£åœ¨é‡æ–°è¿›å…¥æœå¯»â€¦â€¦");
                OpenWXcontent();
            }
        } else {
            toastLog("å½“å‰æœªå¤„äºâ€œå¾®ä¿¡é€šè®¯å½•â€ç•Œé¢\næ­£åœ¨å°è¯•å›åˆ°æ­¤ç•Œé¢");
            sleep(2000);
