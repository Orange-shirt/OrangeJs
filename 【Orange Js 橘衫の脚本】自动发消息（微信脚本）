function InformationSettings() {
    while (true) {
        var DX = dialogs.prompt("è¯·è¾“å…¥è”ç³»åç§°");
        if (DX != "") {
            if (DX != null) {
                break;
            }
        }
    }
    while (true) {
        var XX = dialogs.prompt("è¯·è¾“å…¥è¦å‘é€çš„æ¶ˆæ¯å†…å®¹");
        if (XX != "") {
            if (XX != null) {
                var QR = dialogs.confirm("è¯·ç¡®è®¤ä»¥ä¸‹ä¿¡æ¯", "è”ç³»äººå¤‡æ³¨/ç¾¤èŠåç§°/å…¬ä¼—å·åç§°ï¼š\n" + DX + "\n" + "å°†å‘é€çš„æ¶ˆæ¯å†…å®¹ï¼š" + XX + "\n\nä¹‹åè¿è¡Œå°†é»˜è®¤ä½¿ç”¨æ­¤é…ç½®ï¼Œç¡®å®šä¹‹åå¦‚éœ€æ›´æ”¹è¯·åœ¨è„šæœ¬é…ç½®ä¸­è¿›è¡Œ")
                if (QR == true) {
                    files.createWithDirs("/storage/emulated/0/OrangeJs/è‡ªåŠ¨å¾®ä¿¡å‘æ¶ˆæ¯/æ¶ˆæ¯è®¾ç½®.txt");
                    files.write("/storage/emulated/0/OrangeJs/è‡ªåŠ¨å¾®ä¿¡å‘æ¶ˆæ¯/æ¶ˆæ¯è®¾ç½®.txt", DX + "\n" + XX);
                    break;
                } else {
                    InformationSettings();
                    break;
                }
            }
        }
    }
}

