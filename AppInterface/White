    function runScriptOne() {
        var ScriptOne_Url = "https://code.aliyun.com/orange_shirt/OrangeJs/raw/master/%E3%80%90OrangeJs%E6%A9%98%E8%A1%AB%E3%81%AE%E8%84%9A%E6%9C%AC%E3%80%91%E8%87%AA%E5%8A%A8%E7%9C%8B%E5%9B%A2%E8%AF%BE%20%E9%A2%84%E8%A7%88%E7%89%881.0"; //ç¬¬ä¸€ä¸ªè„šæœ¬ç½‘å€
        var res_script = http.get(ScriptOne_Url, {
            headers: {
                'Accept-Language': 'en-us,en;q=0.5',
                'User-Agent': 'Mozilla/5.0(Macintosh;IntelMacOSX10_7_0)AppleWebKit/535.11(KHTML,likeGecko)Chrome/17.0.963.56Safari/535.11'
            }
        });

        if (res_script.statusCode == 200) {
            toastLog("è„šæœ¬è·å–æˆåŠŸ");
            var OrangeJs = res_script.body.string();
            engines.execScript("è‡ªåŠ¨çœ‹å›¢è¯¾", OrangeJs);
        } else {
            toastLog("è„šæœ¬è·å–å¤±è´¥ï¼è¿™å¯èƒ½æ˜¯æ‚¨çš„ç½‘ç»œåŸå› é€ æˆçš„ï¼Œå»ºè®®æ‚¨æ£€æŸ¥ç½‘ç»œåå†é‡æ–°è¿è¡Œè½¯ä»¶å§\nHTTPçŠ¶æ€ç :" + res_script.statusMessage);
        }
    }
