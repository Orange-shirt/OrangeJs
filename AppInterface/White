function SP() {

    events.removeAllListeners();
    ui.statusBarColor("#2196F3"); //é€šçŸ¥æ é¢œè‰²
    //Not pinkè‰²æ˜¯#DFC8C6
    ui.layout(
        <frame background="#2196F3">//å…¨å±€èƒŒæ™¯é¢œè‰²
            <vertical align="left" paddingTop="5" margin="20 5 20 0">
