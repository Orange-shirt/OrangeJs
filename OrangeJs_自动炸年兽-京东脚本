function clickAndDoing() {

    if (text("å¼€å¿ƒæ”¶ä¸‹").exists()) {
        text("å¼€å¿ƒæ”¶ä¸‹").findOne().click();
        sleep(2000);
    }

    if (text("åšä»»åŠ¡æ‹¿çˆ†ç«¹").exists) {
        var Rk = text("åšä»»åŠ¡æ‹¿çˆ†ç«¹").findOne().bounds();
        click(Rk.centerX(), Rk.centerY());
        toastLog("å·²ç‚¹å‡»äº†â€œåšä»»åŠ¡æ‹¿çˆ†ç«¹â€\nè‹¥æ²¡æœ‰æˆåŠŸå‡ºç°â€œé¢†çˆ†ç«¹â€è¯·é‡è¯•æˆ–åé¦ˆ");
        sleep(3000);
        var hd0 = "å‚åŠ å¥½ç©äº’åŠ¨ (5/5æ¬¡)"
        context_xH = 1;
        while (context_xH == 1) {
            var A = textContains("å‚åŠ å¥½ç©äº’åŠ¨").findOne();
            var AO = A.parent();
            var B = AO.children();
            var H = B[1].text();
            if (H == hd0) {
                toastLog(H);
                context_xH = 0;
            } else {
                toastLog(H);
                B[3].click();
                sleep(3000);
                //è¿”å›
                Justback();
            }
        }


