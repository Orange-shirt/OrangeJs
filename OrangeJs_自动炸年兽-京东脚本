                if (text("é‡æ–°åŠ è½½").exists()) {
                    text("é‡æ–°åŠ è½½").findOnce().click();
                    toastLog("ç‚¹å‡»é‡æ–°åŠ è½½");
                    sleep(1000);
                }
            }
            if (textContains("m.jd.com").exists()) {
                textContains("m.jd.com").findOne().click();
                toastLog("å·²ç‚¹å‡»æœç´¢åå‡ºç°çš„â€œç‚¹æˆ‘ç‚¸å¹´å…½â€æŒ‰é’®\nè‹¥æ²¡æœ‰æˆåŠŸç‚¹å‡»è¿›å…¥æ´»åŠ¨ç•Œé¢è¯·é‡è¯•æˆ–åé¦ˆ");
                for (var deng10s = 10; deng10s > 0; deng10s--) {
                    toastLog("æ­£åœ¨ç­‰å¾…ä¸»æ´»åŠ¨ç•Œé¢åŠ è½½\nå‰©ä½™" + deng10s + "ç§’â€¦â€¦");
                    sleep(2000);
                }
            } else {
                toastLog("å¾ˆæŠ±æ­‰ï¼Œè„šæœ¬å¹¶æ²¡æœ‰é€šè¿‡æœç´¢åæ‰¾åˆ°â€œç‚¹æˆ‘ç‚¸å¹´å…½â€æŒ‰é’®ï¼Œè¯·é‡è¯•æˆ–åé¦ˆ");
            }
