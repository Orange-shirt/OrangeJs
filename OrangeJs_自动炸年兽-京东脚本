function waiting_time() {
    //è®¡æ—¶è¿è¡Œè„šæœ¬
    if (context_i_wait == 0) {
        var Seconds = 60;
        for (Seconds == 60; Seconds > 0; Seconds--) {
            console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
            sleep(1000);
        }
    }
    if (context_i_wait == 1) {
        var Minutes = 4;
        for (Minutes == 4; Minutes >= 0; Minutes--) {
            if (Minutes >= 0) {
                var Seconds = 60;
                for (Seconds == 60; Seconds > 0; Seconds--) {
                    console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                    sleep(1000);
                }
            }
        }
    }
    if (context_i_wait == 2) {
        var Minutes = 9;
        for (Minutes == 9; Minutes >= 0; Minutes--) {
            if (Minutes >= 0) {
                var Seconds = 60;
                for (Seconds == 60; Seconds > 0; Seconds--) {
                    console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                    sleep(1000);
                }
            }
        }
    }
    if (context_i_wait == 3) {
        var Minutes = 29;
        for (Minutes == 29; Minutes >= 0; Minutes--) {
            if (Minutes >= 0) {
                var Seconds = 60;
                for (Seconds == 60; Seconds > 0; Seconds--) {
                    console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                    sleep(1000);
                }
            }
        }
    }
    if (context_i_wait == 4) {
        var Minutes = 59;
        for (Minutes == 59; Minutes >= 0; Minutes--) {
            if (Minutes >= 0) {
                var Seconds = 60;
                for (Seconds == 60; Seconds > 0; Seconds--) {
                    console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                    sleep(1000);
                }
            }
        }
    }
    if (context_i_wait == 5) {
        var Hours = 1;
        for (Hours == 1; Hours >= 0; Hours--) {
            var Minutes = 59;
            for (Minutes == 59; Minutes >= 0; Minutes--) {
                if (Minutes >= 0) {
                    var Seconds = 60;
                    for (Seconds == 60; Seconds > 0; Seconds--) {
                        console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Hours + "å°æ—¶" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                        sleep(1000);
                    }
                }
            }
        }
    }
    if (context_i_wait == 6) {
        var Hours = 2;
        for (Hours == 2; Hours >= 0; Hours--) {
            var Minutes = 59;
            for (Minutes == 59; Minutes >= 0; Minutes--) {
                if (Minutes >= 0) {
                    var Seconds = 60;
                    for (Seconds == 60; Seconds > 0; Seconds--) {
                        console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Hours + "å°æ—¶" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                        sleep(1000);
                    }
                }
            }
        }
    }
    if (context_i_wait == 7) {
        var Hours = 4;
        for (Hours == 4; Hours >= 0; Hours--) {
            var Minutes = 59;
            for (Minutes == 59; Minutes >= 0; Minutes--) {
                if (Minutes >= 0) {
                    var Seconds = 60;
                    for (Seconds == 60; Seconds > 0; Seconds--) {
                        console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Hours + "å°æ—¶" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                        sleep(1000);
                    }
                }
            }
        }
    }
    if (context_i_wait == 8) {
        var Hours = 7;
        for (Hours == 7; Hours >= 0; Hours--) {
            var Minutes = 59;
            for (Minutes == 59; Minutes >= 0; Minutes--) {
                if (Minutes >= 0) {
                    var Seconds = 60;
                    for (Seconds == 60; Seconds > 0; Seconds--) {
                        console.warn("ã€å®šæ—¶è¿è¡Œã€‘è®¡æ—¶ä¸­â€¦â€¦\n" + Hours + "å°æ—¶" + Minutes + "åˆ†é’Ÿ" + Seconds + "ç§’åå¼€å§‹è¿è¡Œ");
                        sleep(1000);
                    }
                }
            }
        }
    }
}

//ä¸‹é¢æ˜¯æ‚¬æµ®çª—
var window = floaty.window(
    <frame>
        <button id="action" text="ç‚¹å‡»åœæ­¢è„šæœ¬" w="120" h="40" bg="#F0EB4336"/>
    </frame>
);
setInterval(() => {}, 1000);
var execution = null;
//è®°å½•æŒ‰é”®è¢«æŒ‰ä¸‹æ—¶çš„è§¦æ‘¸åæ ‡
var x = 0,
    y = 0;
//è®°å½•æŒ‰é”®è¢«æŒ‰ä¸‹æ—¶çš„æ‚¬æµ®çª—ä½ç½®
var windowX, windowY;
//è®°å½•æŒ‰é”®è¢«æŒ‰ä¸‹çš„æ—¶é—´ä»¥ä¾¿åˆ¤æ–­é•¿æŒ‰ç­‰åŠ¨ä½œ
var downTime;
window.action.setOnTouchListener(function(view, event) {
    switch (event.getAction()) {
        case event.ACTION_DOWN:
            x = event.getRawX();
            y = event.getRawY();
            windowX = window.getX();
            windowY = window.getY();
            downTime = new Date().getTime();
            return true;
        case event.ACTION_MOVE:
            //ç§»åŠ¨æ‰‹æŒ‡æ—¶è°ƒæ•´æ‚¬æµ®çª—ä½ç½®
            window.setPosition(windowX + (event.getRawX() - x),
                windowY + (event.getRawY() - y));
            //å¦‚æœæŒ‰ä¸‹çš„æ—¶é—´è¶…è¿‡1.5ç§’åˆ¤æ–­ä¸ºé•¿æŒ‰ï¼Œé€€å‡ºè„šæœ¬
            if (new Date().getTime() - downTime > 1500) {
                toast("é•¿æŒ‰å¯ä»¥ç§»åŠ¨ä½ç½®å“¦ï½");
            }
            return true;
        case event.ACTION_UP:
            //æ‰‹æŒ‡å¼¹èµ·æ—¶å¦‚æœåç§»å¾ˆå°åˆ™åˆ¤æ–­ä¸ºç‚¹å‡»
            if (Math.abs(event.getRawY() - y) < 5 && Math.abs(event.getRawX() - x) < 5) {
                onClick();
            }
            return true;
    }
    return true;
});

function onClick() {
    dialogs.alert("å·²åœæ­¢è¿è¡Œè„šæœ¬ï¼");
    log("ç”¨æˆ·ç‚¹å‡»äº†åœæ­¢æŒ‰é’®");
    engines.stopAllAndToast();
}

function Justback() {
    //ğŸ’ğŸ’ğŸ’ğŸ’ğŸ’ä½¿ç”¨ç”¨æˆ·è®¾å®šçš„è¿”å›æ–¹æ³•
    if (context_i_back == 0) {
        sleep(1000);
        toastLog("ä½¿ç”¨æ™®é€šçš„è¿”å›");
        back();
        sleep(2000);
    }
    if (context_i_back == 1) {
        sleep(1000);
        toastLog("ä½¿ç”¨ROOTè¿”å›\nè¯·ç¡®ä¿å·²ç»™ROOTæƒé™ï¼");
        Back();
        sleep(2000);
    }
    if (context_i_back == 2) {
        openTBandsearch();
        sleep(3000);
        if (currentActivity() != "com.taobao.browser.BrowserActivity") {
            openTBandsearch();
            sleep(3000);
            if (currentActivity() != "com.taobao.browser.BrowserActivity") {
                openTBandsearch_mine();
            }
        }
        jinhuodong();
        click_powerË‰qiandao();
    }
    if (context_i_back == 3) {
        sleep(1000);
        toastLog("ä»å±å¹•ä¸­é—´å‘ä»å·¦å‘å†…æ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [0, height / 2],
            [500, height / 2]
        ]);
        sleep(2000);
    }
    if (context_i_back == 5) {
        sleep(1000);
        toastLog("ä»å±å¹•å·¦ä¾§ä¸‹æ–¹å‘ä¸Šæ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [width / 2 - 300, height - 1],
            [width / 2 - 300, height - 500]
        ]);
        sleep(2000);
    }
    if (context_i_back == 4) {
        sleep(1000);
        toastLog("ä»å±å¹•ä¸­é—´å‘ä»å³å‘å†…æ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [width - 1, height / 2],
            [width - 500, height / 2]
        ]);
        sleep(2000);
    }
    if (context_i_back == 6) {
        sleep(1000);
        toastLog("ä»å±å¹•å·¦ä¾§ä¸‹é¢å‘ä¸Šé¢æ»‘åŠ¨æ¥è¿”å›");
        gestures([context_gestures_speed, [width / 2 + 300, height - 1],
            [width / 2 + 300, height - 500]
        ]);
        sleep(2000);
    }
}

function openJdAndSearch() {
    context_while = 1;
    while (context_while == 1) {
        var time = 1;
        app.startActivity({
            action: "android.intent.action.VIEW", //æ­¤å¤„å¯ä¸ºå…¶ä»–å€¼
            packageName: "com.jingdong.app.mall",
            className: "com.jingdong.app.mall.main.MainActivity"
            //æ­¤å¤„å¯ä»¥åŠ å…¥å…¶ä»–å†…å®¹ï¼Œå¦‚dataã€extras
        });
        toastLog("æ­£åœ¨å°è¯•æ‰“å¼€äº¬ä¸œAPP" + "*" + time);
        sleep(1000);
        if (currentPackage() == "com.jingdong.app.mall") {
            context_while = 0;
        }
    }
    context_while = 1;
    while (context_while == 1) {
        var time = 1;
        app.startActivity({
            action: "android.intent.action.VIEW", //æ­¤å¤„å¯ä¸ºå…¶ä»–å€¼
            packageName: "com.jingdong.app.mall",
            className: "com.jd.lib.search.view.Activity.ProductListActivity"
            //æ­¤å¤„å¯ä»¥åŠ å…¥å…¶ä»–å†…å®¹ï¼Œå¦‚dataã€extras
        });
        toastLog("æ­£åœ¨å°è¯•æ‰“å¼€äº¬ä¸œæœç´¢æ *" + time);
        sleep(1000);
        if (className("android.widget.HorizontalScrollView").exists()) {
            context_while = 0;
        }
    }

    toastLog("æ­£åœ¨æ‰“å¼€â€œäº¬ä¸œâ€APP...");
    if (className("android.widget.HorizontalScrollView").exists()) {
        var x = className("android.widget.HorizontalScrollView").findOne().bounds();
        click(x.centerX(), x.centerY());
        sleep(2000);
        setText(GJCwords);
        sleep(1000);
        text("æœç´¢").waitFor();
        if (text("æœç´¢").exists()) {
            toastLog("ç‚¹å‡»æœç´¢æŒ‰é’®");
            text("æœç´¢").findOne().click();
            for (var deng10s = 10; deng10s > 0; deng10s--) {
                toastLog("æ­£åœ¨ç­‰å¾…æ´»åŠ¨å…¥å£åŠ è½½\nå‰©ä½™" + deng10s + "ç§’â€¦â€¦");
